@model Sitecore.Demo.Feature.JobBoard.Models.DefinitionViewModel


<div class="whole-wrap">
    <div class="container box_1170">
        <div class="section-top-border">
            <h3 class="mb-30">@Model.Title</h3>
            <div class="row">


                @foreach (var item in Model.Definitions)
                {
                    <div class="col-md-4">
                        <div class="single-defination">
                            <h4 class="mb-20">@item.Title</h4>
                            <p>
                                @item.Description
                            </p>
                        </div>
                    </div>
                }



            </div>
        </div>
        </div>
</div>
